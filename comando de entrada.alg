algoritmo "COMANDODEENTRADA"

var
  nome: caractere
  idade: inteiro
  anoAtual: inteiro
inicio
      escreva("Escreva o seu nome: ")
      leia(nome)
      escreva("Indique sua idade: ")
      leia(idade)
      escreva("Em que ano está? ")
      leia(anoAtual)
      escreva("Você nasceu no ano de ", anoAtual-idade)
fimalgoritmo
